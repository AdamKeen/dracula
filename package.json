{
  "author": "Johann Philipp Strathausen",
  "name": "graphdracula",
  "description": "JavaScript Graph Layouting and Drawing",
  "version": "1.0.0",
  "homepage": "http://www.graphdracula.net",
  "scripts": {
    "start": "babel watch src --out-file public/bundle.js",
    "build": "babel src -d dist",
    "watch": "babel --watch src -d dist",
    "test": "mocha --compilers js:babel-register src/**/*.spec.js",
    "test-watch": "mocha --watch --compilers js:babel-register src/**/*.spec.js",
    "check": "eslint src/",
    "example-watch": "watchify -t babelify examples/basic/script.js -v -o examples/basic/bundle.js",
    "publish": "./scripts/publish.sh"
  },
  "keywords": [
    "dracula",
    "graph",
    "svg",
    "math",
    "raphael",
    "snapsvg",
    "dijkstra",
    "bellman-ford"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/strathausen/dracula.git"
  },
  "dependencies": {
    "snapsvg": "^0.4.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "eslint": "^3.2.2",
    "lodash": "^4.14.1",
    "mocha": "~3.0.1",
    "sinon": "^1.17.5",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  }
}
